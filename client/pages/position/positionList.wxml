      <view class="weui-cells__title"> {{storeName}}</view>
      <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell" wx:for="{{positons}}" data-id="{{item.id}}" bindtap='selectOne'>
          <view class="weui-cell__hd">
            <!-- <image src="{{item.img}}" style="margin-right: 5px;vertical-align: middle;width:60px; height: 60px;" mode="aspectFill"></image> -->
          </view>
          <view class="weui-cell__bd">
            <view class="wx-30-text">{{item.no}}</view>
            <view class="wx-30-text wx-text-primary"></view>
          </view>
          <view class="weui-cell__ft weui-cell__ft_in-access ">

          </view>
        </view>
      </view>
    </view>
  </view>
</view>